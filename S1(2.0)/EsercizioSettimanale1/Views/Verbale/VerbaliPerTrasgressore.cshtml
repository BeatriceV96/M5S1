@model List<EsercitazioneSettimanale1.Models.VerbaleEntity>

@{
    ViewData["Title"] = "Verbali per Trasgressore";
}

<h2>Verbali per Trasgressore</h2>

<table class="table">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Cognome</th>
            <th>Totale Verbali</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>             
                <td>@item.Cognome</td>
                <td>@item.Nome</td>
                <td>@item.TotaleVerbali</td>
            </tr>
        }
    </tbody>
</table>
